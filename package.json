{
  "name": "lookup-server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "body-parser": "^1.16.1",
    "express": "4.13.3",
    "faker": "^3.1.0",
    "fs": "0.0.2",
    "morgan": "^1.8.1",
    "mysql": "^2.13.0",
